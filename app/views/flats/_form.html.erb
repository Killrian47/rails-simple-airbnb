<div class="container w-50">
  <%= simple_form_for(flat) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :number_of_guests %>
    <%= f.input :price_per_night %>
    <%= f.input :picture %>
    <%= f.button :submit, class: "btn btn-primary" %>
  <% end %>
</div>